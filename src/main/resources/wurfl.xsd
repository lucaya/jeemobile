<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://www.lucaya.net/wurfl" 
	
	xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:wfl="http://www.lucaya.net/wurfl" 
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
    jaxb:version="2.0"
	elementFormDefault="unqualified" 
	attributeFormDefault="unqualified">

	<annotation>
	  <appinfo>
	    <jaxb:schemaBindings>
	      <jaxb:package name="net.lucaya.jeemobile.wurfl.jaxb"/>
	    </jaxb:schemaBindings>
	  </appinfo>
	</annotation>

	<!-- Root element -->
	<element name="wurfl">
		<complexType>
			<sequence>
				<element name="version" type="wfl:versionType" minOccurs="1" maxOccurs="1" />
				<element name="devices" type="wfl:devicesType" minOccurs="1" maxOccurs="1" />
			</sequence>
		</complexType>
	</element>
	
	<!-- Patch file root element -->
	<element name="wurfl_patch">
		<complexType>
			<sequence>
				<element name="devices" type="wfl:devicesType" minOccurs="1" maxOccurs="1" />
			</sequence>
		</complexType>
	</element>

	<!-- Type for device capabilties -->
	<complexType name="versionType">
		<sequence>
			<element name="ver" type="string" minOccurs="1" maxOccurs="1" />
			<element name="last_updated" type="date" minOccurs="1" maxOccurs="1" />
			<element name="official_url" type="anyURI" minOccurs="1" maxOccurs="1" />
			<element name="maintainers" type="wfl:maintainersType" minOccurs="0" maxOccurs="1" />
			<element name="authors" type="wfl:authorsType" minOccurs="0" maxOccurs="1" />
			<element name="contributors" type="wfl:contributorsType" minOccurs="0" maxOccurs="1" />
			<element name="statement" type="string" minOccurs="0" maxOccurs="1" />
		</sequence>
	</complexType>

	<complexType name="devicesType">
		<sequence>
			<element name="device" minOccurs="1" maxOccurs="unbounded">
				<complexType>
					<sequence>
						<element name="group" type="wfl:groupType" minOccurs="0" maxOccurs="unbounded"/>
					</sequence>
					<attribute name="fall_back" type="string" />
					<attribute name="actual_device_root" type="boolean" use="optional" />
					<attribute name="id" type="string" />
					<attribute name="user_agent" type="string" use="required"/>
				</complexType>
			</element>
		</sequence>
	</complexType>

	<complexType name="groupType">
		<sequence>
			<element name="capability" type="wfl:capabilityType" minOccurs="1" maxOccurs="unbounded"/>
		</sequence>
		<attribute name="id" type="wfl:groupIds" use="required"/>
	</complexType>

	<complexType name="capabilityType">
		<attribute name="name" type="wfl:capability" use="required" />
		<attribute name="value" type="string" use="required" />
	</complexType>

	<!-- Rules for collaborators -->
	<attributeGroup name="collaboratorAttributesGroup">
		<attribute name="name" use="required">
			<simpleType>
				<restriction base="string">
					<whiteSpace value="collapse" />
				</restriction>
			</simpleType>
		</attribute>
		<attribute name="email" type="string" use="required" />
		<attribute name="home_page" type="anyURI" use="optional" />
	</attributeGroup>

	<complexType name="maintainersType">
		<sequence>
			<element name="maintainer" minOccurs="1" maxOccurs="unbounded">
				<complexType>
					<attributeGroup ref="wfl:collaboratorAttributesGroup" />
				</complexType>
			</element>
		</sequence>
	</complexType>

	<complexType name="authorsType">
		<sequence>
			<element name="author" minOccurs="1" maxOccurs="unbounded">
				<complexType>
					<attributeGroup ref="wfl:collaboratorAttributesGroup" />
				</complexType>
			</element>
		</sequence>
	</complexType>

	<complexType name="contributorsType">
		<sequence >
			<element name="contributor" minOccurs="1" maxOccurs="unbounded">
				<complexType>
					<attributeGroup ref="wfl:collaboratorAttributesGroup" />
				</complexType>
			</element>
		</sequence>
	</complexType>


